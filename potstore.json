{
  "version": "2.0",
  "protocol_version": 2,
  "repository": "https://github.com/H0neyBe/honeybee_potstore",
  "last_updated": "2024-12-24",
  "pots": [
    {
      "id": "cowrie",
      "name": "Cowrie",
      "version": "2.9.0",
      "type": "cowrie",
      "description": "Medium to high interaction SSH and Telnet honeypot",
      "protocols": ["ssh", "telnet"],
      "default_ports": {
        "ssh": 2222,
        "telnet": 2223
      },
      "requirements": {
        "python": ">=3.7",
        "os": ["linux", "windows", "macos"]
      },
      "directory": "cowrie",
      "setup": {
        "install": "pip install -r requirements.txt",
        "config": "etc/cowrie.cfg.dist",
        "output_plugin": "src/cowrie/output/honeybee.py"
      },
      "status": "stable",
      "features": [
        "SSH honeypot",
        "Telnet honeypot",
        "Command logging",
        "File download tracking",
        "Session recording",
        "Custom output plugins"
      ],
      "honeybee_integration": {
        "event_port": 9100,
        "event_format": "json",
        "output_plugin": "honeybee.py",
        "auto_configure": true
      }
    },
    {
      "id": "honnypotter",
      "name": "HonnyPotter",
      "version": "1.2.0",
      "type": "honnypotter",
      "description": "WordPress login honeypot - captures brute-force attacks and credential stuffing",
      "protocols": ["http", "https"],
      "default_ports": {
        "http": 80,
        "https": 443
      },
      "requirements": {
        "php": ">=7.4",
        "php_extensions": ["json", "sockets"],
        "os": ["linux", "windows", "macos"]
      },
      "directory": "HonnyPotter",
      "setup": {
        "install": "./install.sh",
        "config": null,
        "forwarder": "honeybee-forwarder.php",
        "entry_point": "standalone.php"
      },
      "status": "stable",
      "features": [
        "WordPress login emulation",
        "Brute-force detection",
        "Credential logging",
        "XML-RPC attack detection",
        "Low resource usage",
        "No database required",
        "Standalone or WordPress plugin mode"
      ],
      "honeybee_integration": {
        "event_port": 9100,
        "event_format": "json",
        "forwarder": "honeybee-forwarder.php",
        "auto_configure": true
      }
    }
  ],
  "coming_soon": [
    {
      "id": "dionaea",
      "name": "Dionaea",
      "type": "dionaea",
      "description": "Low interaction honeypot for multiple protocols",
      "protocols": ["ftp", "http", "smb", "mysql"],
      "status": "planned"
    },
    {
      "id": "kippo",
      "name": "Kippo",
      "type": "kippo",
      "description": "SSH honeypot",
      "protocols": ["ssh"],
      "status": "planned"
    },
    {
      "id": "glastopf",
      "name": "Glastopf",
      "type": "glastopf",
      "description": "Web application honeypot",
      "protocols": ["http", "https"],
      "status": "planned"
    }
  ],
  "installation": {
    "method": "git_clone",
    "default_branch": "main",
    "depth": 1
  },
  "configuration": {
    "supported_config_keys": [
      "ssh_port",
      "telnet_port",
      "http_port",
      "https_port",
      "hostname",
      "log_path",
      "data_path",
      "pot_id"
    ]
  },
  "node_compatibility": {
    "min_version": "1.0.0",
    "protocol": 2
  }
}

